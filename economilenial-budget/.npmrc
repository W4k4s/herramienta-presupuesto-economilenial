# Configuración de seguridad para npm
# Economilenial Budget Plugin

# Configuraciones de audit
audit-level=moderate
fund=false

# Configuraciones de instalación
legacy-peer-deps=true
save-exact=true

# Configuraciones de registry
registry=https://registry.npmjs.org/

# Configuraciones de cache
cache-min=86400

# Configuraciones de progress
progress=true

# Configuraciones de engine-strict
engine-strict=true

# Configuraciones de save-prefix
save-prefix=^

# Configuraciones de optional
optional=true

# Configuraciones de dev
production=false
